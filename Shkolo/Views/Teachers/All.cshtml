@model IEnumerable<AddTeacherFormModel>

@{ViewData["Title"] = "All Teachers";}

<h2 class="display-4">Teachers : </h2>
<div class="row">

    <table class="table">
        <thead>
            <tr>
                <th scope="col">TeacherId</th>
                <th scope="col">Name</th>
            </tr>
        </thead>
        @foreach (var tch in Model)
        {
            <tbody>
                <tr>
                    <td>@tch.TeacherId</td>
                    <td>@tch.Name</td>
                </tr>
            </tbody>
        }
    </table>

</div>
